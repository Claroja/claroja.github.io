{
  "name": "blog",
  "version": "2.0.0",
  "description": "A project of vuepress-theme-hope",
  "type": "module",
  "scripts": {
    "docs:build": "python ./src/.vuepress/code/genIndex.py && vuepress build src",
    "docs:clean-dev": "python ./src/.vuepress/code/genIndex.py && vuepress dev src --clean-cache",
    "docs:dev": "python ./src/.vuepress/code/genIndex.py && vuepress dev src",
    "docs:update-package": "npx vp-update"
  },
  "devDependencies": {
    "@vuepress/client": "2.0.0-rc.0",
    "vue": "^3.3.8",
    "vuepress": "2.0.0-rc.0",
    "vuepress-theme-hope": "2.0.0-rc.0"
  },
  "dependencies": {
    "katex": "^0.16.9"
  }
}
